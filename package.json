{
  "name": "emr-sfn-waiter",
  "version": "0.1.0",
  "description": "CDK library for an SFN workflow that polls for EMR Serverless job completion",
  "author": {
    "name": "Alex Gelman",
    "email": "6887237+alexgelman@users.noreply.github.com"
  },
  "repository": {
    "url": "https://github.com/alexgelman/emr-sfn-waiter.git"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "jsii",
    "build:watch": "jsii --watch",
    "test": "jest",
    "package": "jsii-pacmak"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "18.14.6",
    "aws-cdk-lib": "2.78.0",
    "constructs": "^10.0.0",
    "jest": "^29.5.0",
    "jsii": "^5.0.7",
    "jsii-pacmak": "^1.81.0",
    "ts-jest": "^29.0.5",
